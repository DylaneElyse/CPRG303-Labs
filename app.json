{
  "expo": {
    "name": "test-app-typescript",
    "slug": "test-app-typescript",
    "owner": "Dylane",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-router"
      ]
    ],
    "scheme": "myapp",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.Dylane.testapptypescript"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
